		<setting>
			<conf_id>257</conf_id>
			<conf_title>Files per page options</conf_title>
			<conf_description>Separate values with a ,</conf_description>
			<conf_group>29</conf_group>
			<conf_type>input</conf_type>
			<conf_key>idm_ddfilesperpage</conf_key>
			<conf_value></conf_value>
			<conf_default>5,10,15,20,25</conf_default>
			<conf_extra></conf_extra>
			<conf_evalphp><![CDATA[if ( $save == 1 )
{
    if ( $_POST['idm_ddfilesperpage'] )
    {
        $arr = explode( ",", $_POST['idm_ddfilesperpage'] );
        
        if ( is_array( $arr ) )
        {
            foreach( $arr as $k => $v )
            {
                if( intval( $v ) > 0 )
                {
                    $final_arr[] = $v;
                }
            }
            
            if ( is_array( $final_arr ) )
            {
                $_POST['idm_ddfilesperpage'] = implode( ",", $final_arr );
            }
            else
            {
                $_POST['idm_ddfilesperpage'] = "5,10,15,20,25";
            }
        }
        else
        {
            $_POST['idm_ddfilesperpage'] = "5,10,15,20,25";
        }
    }
    else
    {
        $_POST['idm_ddfilesperpage'] = "5,10,15,20,25";
    }
    
    $key = 'idm_ddfilesperpage';
}]]></conf_evalphp>
